{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pairwise",
  "main": "src/index.ts",
  "compatibility_date": "2024-01-01",
  "durable_objects": {
    "bindings": [
      {
        "name": "ROOM",
        "class_name": "Room"
      }
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Room"]
    }
  ],
  "assets": {
    "directory": "public",
    "binding": "ASSETS",
    "html_handling": "none",
    "not_found_handling": "none"
  }
}